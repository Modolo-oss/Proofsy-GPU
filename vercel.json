{
  "version": 2,
  "buildCommand": "npm run build",
  "framework": null,
  "outputDirectory": "public",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "env": {
    "NUMBERS_API_KEY": "@numbers_api_key"
  }
}
